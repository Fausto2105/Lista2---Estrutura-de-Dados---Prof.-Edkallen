** Observações: Estrutura que representa um estudante
*/

#ifndef ESTUDANTE_H
#define ESTUDANTE_H

#define NUM_NOTAS 3

typedef struct {
    char nome[50];
    int matricula;
    float notas[NUM_NOTAS];
} Estudante;

#endif 
